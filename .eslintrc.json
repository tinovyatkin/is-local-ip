{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:regexp/recommended",
    "plugin:node/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "script"
  },
  "overrides": [
    {
      "files": ["**/__tests__/*.js", "*.test.js", "**/__mocks__/*.js"],
      "env": {
        "jest": true
      },
      "rules": {}
    }
  ]
}
